			"name": "register",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const num = Math.floor(Math.random() * 10000);\r",
							"const id = `Test${num}@mail.com`;\r",
							"pm.globals.set(\"id\", id);\r",
							"pm.globals.set(\"rd\", num);\r",
							"pm.globals.set(\"local\", \"https://localhost:8443\");"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"userId\": \"{{id}}\",\r\n  \"userName\": \"{{rd}}\",\r\n  \"userNickname\": \"테스트계정\",\r\n  \"userPhone\": \"010-9999-9999\",\r\n  \"userPw\": \"ssafy\",\r\n  \"userType\": 1\r\n}",
